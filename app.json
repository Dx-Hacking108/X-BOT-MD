      "value": "DX BOT;DARK-X369"
    },
    "AUDIO_DATA": {
      "description": "Enter Your Owner Name",
      "required": false,
      "value": "DX BOT;DARK-X369;https://i.imgur.com/fVCRCYG.jpeg"
    },
    "BOT_INFO": {
      "description": "Enter Your Owner Number",
      "required": false,
      "value": "DX-BOT;DARK-X369;https://telegra.ph/file/d836b7b1357c1a902c32e.jpg"
